<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
    <property>
        <name>ranger.plugin.obs.service.name</name>
        <value>obs</value>
    </property>
    <!-- ranger policy for anger-obs-service -->
    <property>
        <name>ranger.plugin.obs.policy.cache.dir</name>
        <value>/var/lib/ranger/obs/policy-cache</value>
    </property>
    <property>
        <name>ranger.plugin.obs.policy.pollIntervalMs</name>
        <value>30000</value>
    </property>
    <property>
        <name>ranger.plugin.obs.policy.rest.client.connection.timeoutMs</name>
        <value>120000</value>
    </property>
    <property>
        <name>ranger.plugin.obs.policy.rest.client.read.timeoutMs</name>
        <value>30000</value>
    </property>
    <property>
        <name>ranger.plugin.obs.policy.rest.supports.policy.deltas</name>
        <value>false</value>
    </property>
    <!-- ranger admin restful http  Enter Ranger Admin Url-->
    <property>
        <name>ranger.plugin.obs.policy.rest.url</name>
        <value>http://192.168.0.169:6080</value>
    </property>
    <property>
        <name>ranger.plugin.obs.policy.source.impl</name>
        <value>org.apache.ranger.admin.client.RangerAdminRESTClient</value>
    </property>
</configuration>
